      "h3": {}
    },
    "switches": {
      "s1": {
        "cli_input": "s1-commands.txt",
        "program": "simple_router.p4"
      },
      "s2": {
        "cli_input": "s2-commands.txt",
        "program": "simple_router.p4"
      },
      "s3": {
        "cli_input": "s3-commands.txt",
        "program": "simple_router.p4"
      }
    }
  }
